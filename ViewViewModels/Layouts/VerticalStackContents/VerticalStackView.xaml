<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyFirstMobileApp.ViewViewModels.Layouts.VerticalStackContents.VerticalStackView"
             Title="VerticalStackView">

    <StackLayout>

        <StackLayout>

            <BoxView WidthRequest="90"
                     HeightRequest="90"
                     Color="Red" />
            <BoxView WidthRequest="90"
                     HeightRequest="90"
                     Color="Green" />
            <BoxView WidthRequest="90"
                     HeightRequest="90"
                     Color="Blue" />
            <BoxView HorizontalOptions="EndAndExpand"
                     WidthRequest="90"
                     HeightRequest="90"
                     Color="Purple" />
        </StackLayout>


    </StackLayout>


</ContentPage>